{
  "name": "jsonschema-to-runtypes",
  "version": "1.0.0",
  "repository": "github:cobraz/jsonschema-to-runtypes",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsnd --respawn src/main.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage src/**/*",
    "test:watch": "jest --watch",
    "build": "tsc",
    "generate:docs": "typedoc --readme none --out docs src"
  },
  "lint-staged": {
    "src/**/*.{yaml,yml}": "prettier --write",
    "src/**/*.{js,ts}": "eslint --cache --fix",
    "{.eslintrc}": "prettier --write",
    "*.md": "prettier --write"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.7",
    "generate-runtypes": "^1.1.1",
    "glob-promise": "^4.1.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "mz": "^2.7.0",
    "runtypes": "^5.0.2",
    "stdin": "^0.0.1",
    "ts-morph": "^10.0.1",
    "typescript": "~4.1.3"
  },
  "devDependencies": {
    "@types/faker": "^5.1.7",
    "@types/jest": "~26.0.21",
    "@types/json-schema": "^7.0.7",
    "@types/node": "~14.14.35",
    "@typescript-eslint/eslint-plugin": "~4.18.0",
    "@typescript-eslint/parser": "~4.18.0",
    "eslint": "~7.22.0",
    "eslint-config-prettier": "~8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "~24.3.2",
    "faker": "^5.4.0",
    "husky": "^5.1.3",
    "jest": "~26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "~26.5.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typedoc": "^0.22.18",
    "typedoc-plugin-markdown": "^3.0.8"
  }
}